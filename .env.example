AMAZON_EMAIL=
AMAZON_PASSWORD=
ASIN=

# AI Provider: 'openai' or 'ollama'
AI_PROVIDER=ollama

# OpenAI Configuration (only needed if AI_PROVIDER=openai)
OPENAI_API_KEY=

# Ollama Configuration (only needed if AI_PROVIDER=ollama)
OLLAMA_BASE_URL=http://localhost:11434
# Vision model for image transcription (must support vision)
# Examples: qwen2.5-vl:32b, llama3.2-vision, llava:7b, llava:13b, llava:34b, minicpm-v, moondream
OLLAMA_VISION_MODEL=qwen2.5-vl:7b
# Concurrency: number of images to process in parallel (1-16, default: 16)
# Lower values (1-4) for large models or unstable servers, higher values (12-16) for smaller/faster models
OLLAMA_CONCURRENCY=16
